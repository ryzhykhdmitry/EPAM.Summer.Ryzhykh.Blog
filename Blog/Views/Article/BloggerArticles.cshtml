@model IEnumerable<Blog.Models.ArticleViewModel.ArticleViewModel>
<div class="container-fluid"><div class="row text-center"><h4>User's blog</h4></div></div>
<div class="col-md-offset-2 col-md-8 col-lg-offset-2 col-lg-8" id="right_content">
    
    @if (Model.Count() != 0)
            {
        @Html.Action("ShowPages", "Article", new { models = Model })        
    }
    else
    {
        <h3>У блоггера нет статей</h3>
    }
</div>